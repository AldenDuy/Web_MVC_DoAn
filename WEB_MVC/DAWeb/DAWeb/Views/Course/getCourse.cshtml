@model IEnumerable<DAWeb.Models.Course>
@{
    ViewBag.Title = "getCourse";
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}


    <div class="container py-md-4 text-center">
        <h3 class="tittle-w3layouts two">Lesson Library Online - Lessons by Course</h3>
        
            @foreach (var i in Model)
            {


                <div class="team-img"><img src="~/Content/images/@i.Images" alt="news image" class="img-fluid"> </div>
                <div class="team-info">
                    <h4>@i.Name</h4>
                    <small>@i.Level</small>
                    <p><a href="#">@i.Descriptions</a></p>
                </div>
                <br />


            }
       
        </div>

