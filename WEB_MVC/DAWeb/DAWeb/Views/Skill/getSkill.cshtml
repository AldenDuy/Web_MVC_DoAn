@model IEnumerable<DAWeb.Models.Skill>

@{
    ViewBag.Title = "getSkill";
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}
<!DOCTYPE html>
<div class="container py-md-4 text-center">
    <h3 class="tittle-w3layouts two">Lesson Library Online - Lessons by Skill</h3>
    @foreach (var i in Model)
    {

        <div class="col-lg-4 team-main">
            <div>
                <audio controls>
                    <source src="/Content/audio/@i.Media" type="	audio/mpeg">
                </audio>
            </div>
            <div class="team-info">
                <h4>@i.Name</h4>
                <p>@i.Descriptions</p>
            </div>
            <br />
        </div>

    }
</div>
