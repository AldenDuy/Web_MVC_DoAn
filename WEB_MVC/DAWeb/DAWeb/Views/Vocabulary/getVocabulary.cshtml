@model IEnumerable<DAWeb.Models.Vocabulary>

@{
    ViewBag.Title = "getVocabulary";
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}

    <div class="container py-md-4 text-center">
        <h3 class="tittle-w3layouts two">Lesson Library Online - Lessons by Vocabulary</h3>

        @foreach (var i in Model)
        {


            <div class="team-img"><img src="~/Content/images/@i.Images" alt="news image" class="img-fluid"> </div>
            <div class="team-info">
                <h4>@i.Name</h4>
                <p><a href="#">@i.Descriptions</a></p>
            </div>
            <br />


        }

    </div>